{
  "name": "petfinder-graphql-server",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "types": "index.d.ts",
  "scripts": {
    "prebuild:dev": "rimraf dist",
    "prebuild": "rimraf dist",
    "dev": "NODE_ENV=development concurrently \"yarn build:dev\" \"yarn start:dev\"",
    "build:dev": "tsc --watch --preserveWatchOutput",
    "start:dev": "nodemon --inspect dist/index.js",
    "build": "tsc",
    "start": "node dist/index.js",
    "fmt": "prettier \"**/*.{js,jsx,ts,tsx,json}\" --write",
    "lint": "eslint . --ext .js,.ts",
    "test:prettier": "prettier --check \"**/*.{js,jsx,ts,tsx,json}\"",
    "test": "yarn test:prettier && yarn lint && yarn build",
    "update:schema": "yarn update:_schema && yarn update:_types",
    "update:_schema": "ts-node src/scripts/update-schema",
    "update:_types": "graphql-codegen && prettier index.d.ts --write"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "apollo-server-express": "^2.13.1",
    "express": "^4.17.1",
    "graphql": "^15.0.0",
    "immutable": "^4.0.0-rc.12",
    "isomorphic-unfetch": "^3.0.0",
    "node-cache": "^5.1.2",
    "query-string": "^6.13.2"
  },
  "devDependencies": {
    "@babel/cli": "^7.11.6",
    "@babel/core": "^7.11.6",
    "@babel/node": "^7.10.5",
    "@babel/preset-env": "^7.11.5",
    "@graphql-codegen/cli": "^1.19.4",
    "@graphql-codegen/typescript": "^1.19.0",
    "@types/node-cache": "^4.2.5",
    "@typescript-eslint/eslint-plugin": "^4.9.0",
    "@typescript-eslint/parser": "^4.9.0",
    "concurrently": "^5.3.0",
    "eslint": "^7.15.0",
    "nodemon": "^2.0.4",
    "prettier": "^2.0.5",
    "rimraf": "^3.0.2",
    "ts-node": "^9.1.0",
    "typescript": "^4.1.2"
  }
}
